prog:source.o source_lot1.o source_lot2.o source_lot3.o source_lot4.o source_lot5.o main.o source_sous_menu.o
	gcc source.o source_lot1.o source_lot2.o source_lot3.o source_lot4.o source_lot5.o source_sous_menu.o main.o -o prog -g -lSDL -lSDL_image -lSDL_ttf -lSDL_mixer
