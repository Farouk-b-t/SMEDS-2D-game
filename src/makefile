	gcc -c source_lot3.c 
source_lot4.o:source_lot4.c
	gcc -c source_lot4.c 
source_lot5.o:source_lot5.c
	gcc -c source_lot5.c 
source_sous_menu.o:source_sous_menu.c
	gcc -c source_sous_menu.c 
